<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout14"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminGetUserListFragment" >

    <EditText
        android:id="@+id/userListSearch"
        android:layout_width="242dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Plate number search..."
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.301"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <Button
        android:id="@+id/userListSearchBtn"
        android:layout_width="84dp"
        android:layout_height="63dp"
        android:layout_marginStart="4dp"
        app:cornerRadius="300dp"
        app:icon="@drawable/baseline_search_24"
        app:iconSize="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/userListSearch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/userListSearch"
        app:layout_constraintTop_toTopOf="@+id/userListSearch"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/userListRecyclerView"
        android:layout_width="387dp"
        android:layout_height="561dp"
        android:isScrollContainer="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>