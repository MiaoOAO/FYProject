<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminVisitorListFragment">

    <View
        android:id="@+id/view6"
        android:layout_width="416dp"
        android:layout_height="367dp"
        android:background="#BCEABE"
        android:rotation="360"
        android:rotationX="-180"
        android:rotationY="-34"
        android:scaleX="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="416dp"
        android:layout_height="367dp"
        android:layout_marginTop="400dp"
        android:background="#BBE1F3"
        android:rotation="144"
        android:rotationX="210"
        android:rotationY="-171"
        android:scaleX="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/visListSearchAdmin"
        android:layout_width="242dp"
        android:layout_height="40dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/border"
        android:ems="10"
        android:hint="Plate number search..."
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/visListSearchBtnAdmin"
        android:layout_width="84dp"
        android:layout_height="63dp"
        app:cornerRadius="300dp"
        app:icon="@drawable/baseline_search_24"
        app:iconSize="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/visListSearchAdmin"
        app:layout_constraintStart_toEndOf="@+id/visListSearchAdmin"
        app:layout_constraintTop_toTopOf="@+id/visListSearchAdmin" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/visListRecyclerViewAdmin"
        android:layout_width="380dp"
        android:layout_height="579dp"
        android:layout_marginTop="32dp"
        android:isScrollContainer="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visListSearchAdmin"
        tools:listitem="@layout/visitor_list_layout" />
</androidx.constraintlayout.widget.ConstraintLayout>