<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout9"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminVisitorDetailsFragment">

    <View
        android:id="@+id/view6"
        android:layout_width="416dp"
        android:layout_height="367dp"
        android:background="#BCEABE"
        android:rotation="360"
        android:rotationX="-180"
        android:rotationY="-34"
        android:scaleX="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="416dp"
        android:layout_height="367dp"
        android:layout_marginTop="400dp"
        android:background="#BBE1F3"
        android:rotation="144"
        android:rotationX="210"
        android:rotationY="-171"
        android:scaleX="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewDetailsAdmin"
        android:layout_width="381dp"
        android:layout_height="487dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view6" />

    <TextView
        android:id="@+id/visDetailsNameAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VisitorName"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView9Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView9Admin" />

    <TextView
        android:id="@+id/visDetailsPlateAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VisitorPlate"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView12Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView12Admin" />

    <TextView
        android:id="@+id/visDetailsVisitDateAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VisitDate"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView13Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView13Admin" />

    <TextView
        android:id="@+id/textView9Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Visitor's Name"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView11Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView11Admin" />

    <TextView
        android:id="@+id/textView11Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Visitor Information"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Visitor's Plate Number"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/visDetailsNameAdmin"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsNameAdmin" />

    <ImageView
        android:id="@+id/imageView8Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_check_box_24"
        app:layout_constraintEnd_toStartOf="@+id/textView14Admin"
        app:layout_constraintTop_toTopOf="@+id/textView14Admin" />

    <ImageView
        android:id="@+id/imageView6Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_phone_android_24"
        app:layout_constraintEnd_toStartOf="@+id/textView15Admin"
        app:layout_constraintTop_toTopOf="@+id/textView15Admin" />

    <TextView
        android:id="@+id/textView13Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Visitor's Visit Date"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView12Admin"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsPlateAdmin" />

    <TextView
        android:id="@+id/textView14Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Check-in Time"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/visDetailsPhoneAdmin"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsPhoneAdmin" />

    <TextView
        android:id="@+id/visCheckinAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="visCheckin"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView14Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView14Admin" />

    <TextView
        android:id="@+id/textView15Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Visitor's Phone"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/visDetailsVisitDateAdmin"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsVisitDateAdmin" />

    <TextView
        android:id="@+id/visDetailsPhoneAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VisitorPhone"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView15Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView15Admin" />

    <Button
        android:id="@+id/visDetailsDelButtonAdmin"
        android:layout_width="161dp"
        android:layout_height="49dp"
        android:layout_marginTop="116dp"
        android:text="Delete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visCheckinAdmin" />

    <ImageView
        android:id="@+id/imageView4Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_person_24"
        app:layout_constraintEnd_toStartOf="@+id/textView9Admin"
        app:layout_constraintTop_toTopOf="@+id/textView9Admin" />

    <ImageView
        android:id="@+id/imageView5Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_directions_car_24"
        app:layout_constraintEnd_toStartOf="@+id/textView12Admin"
        app:layout_constraintTop_toTopOf="@+id/textView12Admin" />

    <ImageView
        android:id="@+id/imageView7Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_date_range_24"
        app:layout_constraintEnd_toStartOf="@+id/textView13Admin"
        app:layout_constraintTop_toTopOf="@+id/textView13Admin" />

    <ImageView
        android:id="@+id/imageView17Admin"
        android:layout_width="38dp"
        android:layout_height="45dp"
        android:src="@drawable/baseline_person_add_24"
        app:layout_constraintEnd_toStartOf="@+id/textView26Admin"
        app:layout_constraintTop_toTopOf="@+id/textView26Admin" />

    <TextView
        android:id="@+id/visDetailsResidentAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="admin"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView26Admin"
        app:layout_constraintTop_toBottomOf="@+id/textView26Admin" />

    <TextView
        android:id="@+id/textView26Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Resident Owner"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView14Admin"
        app:layout_constraintTop_toBottomOf="@+id/visCheckinAdmin" />

    <Button
        android:id="@+id/parkingResCancelBtnAdmin"
        android:layout_width="210dp"
        android:layout_height="51dp"
        android:layout_marginTop="8dp"
        android:text="Cancel Parking Reserve"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsDelButtonAdmin"
        app:layout_constraintVertical_bias="0.108" />

    <TextView
        android:id="@+id/visDetailsResidentPhoneAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="admin"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/visDetailsResidentAdmin"
        app:layout_constraintTop_toBottomOf="@+id/visDetailsResidentAdmin" />

</androidx.constraintlayout.widget.ConstraintLayout>