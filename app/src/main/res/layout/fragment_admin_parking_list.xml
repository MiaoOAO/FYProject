<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout13"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminParkingListFragment" >

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/parkingRecyclerViewAdmin"
        android:layout_width="381dp"
        android:layout_height="562dp"
        android:layout_marginTop="152dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/parkingSearchAdmin"
        android:layout_width="272dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:hint="plate number search..."
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/parkingSearchBtnAdmin"
        android:layout_width="65dp"
        android:layout_height="50dp"
        app:icon="@drawable/baseline_search_24"
        app:iconSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/parkingSearchAdmin"
        app:layout_constraintStart_toEndOf="@+id/parkingSearchAdmin"
        app:layout_constraintTop_toTopOf="@+id/parkingSearchAdmin" />
</androidx.constraintlayout.widget.ConstraintLayout>